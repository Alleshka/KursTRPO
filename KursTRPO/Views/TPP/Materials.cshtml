@model IEnumerable<KursTRPO.Models.Material>

@{
    ViewBag.Title = "ViewListMaterial";
}

<h2>Список материалов</h2>
<table width="100%" border="1">
    <tr align="center">
        <td>Код материала</td>
        <td>Название</td>
        <td>Сортамент</td>
        <td>Печать</td>
        <td>Обозначение стандарта</td>
    </tr>
    @foreach (KursTRPO.Models.Material temp in Model)
    {
        <tr align="center">
            <td>@temp.MaterialId</td>
            <td>@temp.Name</td>
            <td>@temp.Assortment</td>
            <td>@temp.Stamp</td>
            <td>@temp.DesignOfStandard</td>
        </tr>
    }
</table>